<!DOCTYPE html>
<html>
<body>

<p id="demo"></p>

<script>
var cars = ["BMW", "Volvo", "Saab", "Ford"];
var text = "";
var i;
for (i = 0; i < cars.length; i++) {
       if(cars[i] == "Ford"){
    text += cars[i] + "<br>";
 }
else{
   if(cars[i] != "Ford"){
   text = "follow";
 }
}
}

document.getElementById("demo").innerHTML = text;
</script>

</body>
</html>
